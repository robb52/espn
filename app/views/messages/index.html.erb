<% @messages.each do |message| %>
  <div class="container">
    <div class="row">
      <div class="col-md-10 offset-md-1">
        <div class="message">
          <h2 class="message-title"><%= message.title %></h2>
          <%= link_to "Join In", message, class: "btn btn-primary" %> 
        </div>
      </div>  
    </div>
  </div>
<% end %>
 